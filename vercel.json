{
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "dev": {
    "framework": "parcel",
    "distDir": "dist",
    "command": "pnpm start"
  },
  "installCommand": "pnpm install",
  "buildCommand": "parcel build public/index.html",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
